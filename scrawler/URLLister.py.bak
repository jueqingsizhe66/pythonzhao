from sgmllib import SGMLParser

class URLLister(SGMLParser): #用SGMLParser 来处理HTML
    
    def reset(self):
        SGMLParser.reset(self)
        self.urls = []

    
    def start_a(self, attrs):  #这里找到所有的标签<a>
        href = [v for k,v in attrs if k == 'href']
        
        if href:
            self.urls.extend(href)


